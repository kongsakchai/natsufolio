{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"importOrder": [
		"^svelte",
		"^\\$app/(.*)$",
		"^\\$lib/assets/(.*)$",
		"^\\$lib/components/(.*)$",
		"^\\$lib/data/(.*)$",
		"^\\$lib/use/(.*)$",
		"^\\$lib/utils/(.*)$",
		"^\\$lib/transitions/(.*)$",
		"^\\$lib/types$",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-tailwindcss",
		"@trivago/prettier-plugin-sort-imports"
	],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
